from appyratus.validation import fields
from pybiz import BizObject


class {{ biz.name|camel }}(BizObject):
    """
    # {{ biz.name|camel }}
    The {{ biz.name|title }} business object.
{% if biz.fields %}

    ## Fields
{% for field in biz.fields %}
    - `{{ field['name'] }}`: TODO 
{% endfor %}
{% endif %}
    """

{% if biz.fields %}
{% for field in biz.fields %}
    {{ field['name'] }} = fields.{{ field['type'] }}()
{% endfor %}
{% else %}
    pass
{% endif %}
